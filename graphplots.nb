Cell["\<\
The x and y position of each atom (we have A,B and C in this case) are \
determined from the primitive lattice vectors of your system. We assign a \
label ii = m(x)+(n(y)-1)*l for each atom and construct the full lattice list \
\[OpenCurlyDoubleQuote]plist\[CloseCurlyDoubleQuote]:\
\>", "Text",
 CellChangeTimes->{{3.811956000007224*^9, 
  3.811956084677417*^9}},ExpressionUUID->"b3127f10-84ed-45c9-9472-\
e7de1a012540"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"plist", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "1", ",", 
       RowBox[{"3", "*", "ndim"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"ii", "=", 
      RowBox[{"m", "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"n", "-", "1"}], ")"}], "*", "l"}]}]}], ";", 
     RowBox[{"xa", "=", 
      RowBox[{"(", 
       RowBox[{"m", "+", 
        FractionBox["n", "2"]}], ")"}]}], ";", 
     RowBox[{"ya", "=", 
      RowBox[{
       FractionBox[
        SqrtBox["3"], "2"], "*", "n"}]}], ";", 
     RowBox[{"xb", "=", 
      RowBox[{"xa", "+", 
       FractionBox["1", "2"]}]}], ";", 
     RowBox[{"yb", "=", "ya"}], ";", 
     RowBox[{"xc", "=", 
      RowBox[{"xa", "+", 
       FractionBox["1", "4"]}]}], ";", 
     RowBox[{"yc", "=", 
      RowBox[{"ya", "+", 
       FractionBox[
        SqrtBox["3"], "4"]}]}], ";", 
     RowBox[{
      RowBox[{"plist", "[", 
       RowBox[{"[", 
        RowBox[{"ii", ",", "1"}], "]"}], "]"}], "=", "xa"}], ";", 
     RowBox[{
      RowBox[{"plist", "[", 
       RowBox[{"[", 
        RowBox[{"ii", ",", "2"}], "]"}], "]"}], "=", "ya"}], ";", 
     RowBox[{
      RowBox[{"plist", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"ii", "+", "ndim"}], ",", "1"}], "]"}], "]"}], "=", "xb"}], 
     ";", 
     RowBox[{
      RowBox[{"plist", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"ii", "+", "ndim"}], ",", "2"}], "]"}], "]"}], "=", "yb"}], 
     ";", 
     RowBox[{
      RowBox[{"plist", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"ii", "+", "ndim", "+", "ndim"}], ",", "1"}], "]"}], "]"}], 
      "=", "xc"}], ";", 
     RowBox[{
      RowBox[{"plist", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"ii", "+", "ndim", "+", "ndim"}], ",", "2"}], "]"}], "]"}], 
      "=", "yc"}]}], ",", 
    RowBox[{"{", 
     RowBox[{"n", ",", "1", ",", "l"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"m", ",", "1", ",", "l"}], "}"}]}], "]"}], ";"}]}], "Input",
