Cell["\<\
The connect table shall be the one with the information of each next neighbor \
connection.  \[OpenCurlyDoubleQuote]Dmin\[CloseCurlyDoubleQuote] and \
\[OpenCurlyDoubleQuote]Dmax\[CloseCurlyDoubleQuote] are defined from your \
lattice spacing \[OpenCurlyDoubleQuote]a\[CloseCurlyDoubleQuote] and due to \
numerical fluctuations we do not have dist = a exactly. In our case, the \
distance between atoms in the same unit  cell is a/2=0.5:\
\>", "Text",
 CellChangeTimes->{{3.811955706138741*^9, 3.811955819472871*^9}, {
  3.81195608834149*^9, 
  3.811956088813163*^9}},ExpressionUUID->"33c2a8ab-0700-42f4-8944-\
021129c1e658"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"connect", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"3", "*", "ndim"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", 
       RowBox[{"3", "*", "ndim"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"nz", "=", "3"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"vneigh", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"3", "*", "ndim"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "nz"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", 
  RowBox[{
   RowBox[{
    RowBox[{"ix", "=", 
     RowBox[{"plist", "[", 
      RowBox[{"[", 
       RowBox[{"i", ",", "1"}], "]"}], "]"}]}], ";", 
    RowBox[{"iy", "=", 
     RowBox[{"plist", "[", 
      RowBox[{"[", 
       RowBox[{"i", ",", "2"}], "]"}], "]"}]}], ";", 
    RowBox[{"Do", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"jx", "=", 
        RowBox[{"plist", "[", 
         RowBox[{"[", 
          RowBox[{"j", ",", "1"}], "]"}], "]"}]}], ";", 
       RowBox[{"jy", "=", 
        RowBox[{"plist", "[", 
         RowBox[{"[", 
          RowBox[{"j", ",", "2"}], "]"}], "]"}]}], ";", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"dist", "=", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"ix", "-", "jx"}], ")"}], "^", "2"}], "+", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"iy", "-", "jy"}], ")"}], "^", "2"}]}], "]"}]}], ";", 
          RowBox[{"dmin", "<", "dist", "<", "dmax"}]}], ",", 
         RowBox[{
          RowBox[{"connect", "[", 
           RowBox[{"[", 
            RowBox[{"i", ",", "j"}], "]"}], "]"}], "=", "1"}]}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", 
        RowBox[{"3", "*", "ndim"}]}], "}"}]}], "]"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"i", ",", "1", ",", 
     RowBox[{"3", "*", "ndim"}]}], "}"}]}], "]"}]}], "Input",
 CellChangeTimes->{
